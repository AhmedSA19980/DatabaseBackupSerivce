<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	
	<connectionStrings>

		<add name="DbConn"
		connectionString="Server=.;Database=EmployeesDB;Integrated Security=True;TrustServerCertificate=True;" />
	</connectionStrings>
	
	<appSettings>
	<add key="BackupFolder" value="D:\DatabaseBackups" />
	<add key="LogFolder" value="D:\DatabaseBackups\Logs" />
	<add key="BackupIntervalMinutes" value="5" />
	</appSettings>
	
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
</configuration>